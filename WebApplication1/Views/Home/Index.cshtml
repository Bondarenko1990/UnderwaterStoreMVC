}

<div id="modDialog" class="modal fade bd-example-modal-lg">
    <div id="dialogContent" class="modal-dialog modal-dialog-centered"></div>
</div>
<div class="row">
    <div class="col-xl-12 d-none d-md-block">
        <div class="w3-content w3-section mx-auto" style="max-width:500px; height:350px;">
            <img class="mySlides w3-animate-fading" src="~/Images/sudaka.jpg" style="width:auto;height:100%">
            <img class="mySlides w3-animate-fading" src="~/Images/sudak2.jpg" style="width:auto;height:100%">
            <img class="mySlides w3-animate-fading" src="~/Images/som2.jpg" style="width:auto;height:100%">
            <img class="mySlides w3-animate-fading" src="~/Images/som1.jpg" style="width:auto;height:100%">
            <img class="mySlides w3-animate-fading" src="~/Images/schuka1.jpg" style="width:auto;height:100%">
            <img class="mySlides w3-animate-fading" src="~/Images/sazan1.jpg" style="width:auto;height:100%">
        </div>
    </div>
</div>

<div class="ramka">
    <div class="row">
        <div class="col-xl-4 col-lg-6 m-x-auto">
            <div class="akcii">
                <img class="d-block img-fluid" src="~/Images/skydkaNaSumku50.png">

                <div id="foto">
                    <img class="d-block img-fluid" src="~/Images/akcii2.jpg" />
                </div>
                <h5>Получи сумку за пол цены</h5>
                 @using (Html.BeginForm("Akciya1", "Home", new { }))
                    {
                     <button class="btn btn-success" type="submit">Подробнее</button>
                    }
            </div>
        </div>
        <div class="col-xl-4 col-lg-6 m-x-auto">
            <div class="akcii">
                <img class="d-block img-fluid" src="~/Images/komplektPoCene.png">

                <div id="foto">
                    <img class="d-block img-fluid" src="~/Images/akcii1.jpg" />
                </div>
                <h5>Забирай комплект по цене ружья</h5>
                @using (Html.BeginForm("Akciya2", "Home", new { }))
                    {
